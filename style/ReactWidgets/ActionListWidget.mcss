.list {
    list-style: none;
    margin: 0;
    padding: 0;
    width: 100%;
}

.item {
    color: #333;
    padding: 5px;
    cursor: pointer;

    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.activeItem {
  composes: item;
  background: A3CEFF;
  color: 4A4A4A;
}

.item:hover {
    background: #333;
    color: #eee;
}
